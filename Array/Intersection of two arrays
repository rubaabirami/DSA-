//Time Complexity: O(n log n + m log n)

//space Complexity: O(n) (for storing s1 and intersection)
#include<iostream>
#include<set>
using namespace std;
int main(){
    int n,m;
    set<int>s;
    cout<<"Enter the size of arr1=";
    cin>>n;
     cout<<"Enter the size of arr2=";
     cin>>m;
     int arr1[n];
     int arr2[m];
     cout<<"enter "<<n<<" elements=";
     for(int i=0;i<n;i++){
         cin>>arr1[i];
         s.insert(arr1[i]);
     }
     cout<<"enter "<<m<<" elements=";
     for(int i=0;i<m;i++){
         cin>>arr2[i];
         s.insert(arr2[i]);
     }
     cout<<"Intersection of two array is=";
     for(int i=0;i<n;i++){
         for(int j=0;j<m;j++){
             if(arr1[i]==arr2[j]){
                 cout<<arr2[j]<<" ";
             }
         }
     }
}
