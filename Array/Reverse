#include <iostream>
#include <climits>
using namespace std;

// Time Complexity: O(n)
// Space Complexity: O(1)
void reverse(int arr[],int n){
     for(int i=0;i<n;i++){
         int st=0,end=n-1;
         while(st<end){
             swap(arr[st],arr[end]);
             st++;
             end--;
         }
         }
     }
    

int main() {
    int n;
    cout<<"Enter number of array size=";
    cin>>n;
    
    int arr[n],target;
    cout << "Enter " << n << " elements:\n";
    for(int i=0;i<n;i++)
    cin>>arr[i];
    
     
        reverse(arr,n);
        cout<<"reverse of array is=";
            for(int i=0;i<n;i++){
                
                cout<<arr[i];
            }
    return 0;
}
