#include <iostream>
#include <climits>
using namespace std;

// Time Complexity: O(n)
// Space Complexity: O(1)
int main() {
    int n;
    cout<<"Enter number of array size=";
    cin>>n;
    
    int arr[n];
    cout << "Enter " << n << " elements:\n";
    for(int i=0;i<n;i++)
    cin>>arr[i];
    int sum=0,pro=1;
     for(int i=0;i<n;i++){
         sum=sum+arr[i];
         pro=pro*arr[i];
         
     }
     cout<<"Sum is="<<sum<<endl;
     cout<<"pro is="<<pro<<endl;

    return 0;
}
