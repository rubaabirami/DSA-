//Time complexity=O(n)
//Space Complexity=(1)

#include <iostream>
using namespace std;

int main() {

    int n;
    int zeroCount = 0;
    int oneCount = 0;
    int twoCount = 0;
    cout<<"enter size=";
    cin>>n;
    int a[n];
    cout<<"Enter "<<n<<" elements=";
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    
    for(int i=0;i<=n-1;i++){
        
        if (a[i] == 0)
            zeroCount++;
        
        else if (a[i] == 1)
            oneCount++;
            
        else
            twoCount++;
    }
    
    for(int i=0;i<zeroCount;i++){
        a[i] = 0;
    }
    
    for(int i=zeroCount;i<(zeroCount + oneCount);i++){
        a[i] = 1;
    }
    
    for(int i=(zeroCount + oneCount);i<n;i++){
        a[i] = 2;
    }
    
    for(int i=0;i<n;i++){
        cout<<a[i]<<" ";
    }
    
	return 0;
}
