#include <iostream>
#include <climits>
using namespace std;

// Time Complexity: O(n)
// Space Complexity: O(1)
int linearSearch(int arr[],int n,int target){
     for(int i=0;i<n;i++){
         if(arr[i]==target){
         return i;
         }
     }
     return -1;
}
int main() {
    int n;
    cout<<"Enter number of array size=";
    cin>>n;
    
    int arr[n],target;
    cout << "Enter " << n << " elements:\n";
    for(int i=0;i<n;i++)
    cin>>arr[i];
    cout<<"Enter target=";
    cin>>target;
     
        cout<<linearSearch(arr,n,target)<<endl;
    return 0;
}
