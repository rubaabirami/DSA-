#include <iostream>
using namespace std;

// Time Complexity: O(n)
// Space Complexity: O(1)
int unique(int arr[],int n){
    int sol=0;
    for(int i=0;i<n;i++){
        sol=sol^ arr[i];
    }
    return sol;
}
int main() {
    int n;
    cout<<"Enter number of array size=";
    cin>>n;
    
    int arr[n];
    cout << "Enter " << n << " elements:\n";
    for(int i=0;i<n;i++)
    cin>>arr[i];
   cout<<"unique element is="<<unique(arr,n);
    return 0;
}
