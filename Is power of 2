#include <iostream>
#include <cmath>
using namespace std;

// Time Complexity: O(1)
// Space Complexity: O(1)
bool is_power_of_two(int n) {
    if (n <= 0)
        return false;
    return (ceil(log2(n)) == floor(log2(n)));
}

int main() {
    int n;
    cout << "Enter num = ";
    cin >> n;

    if (is_power_of_two(n))
        cout << "Num is power of two";
    else
        cout << "Num is not power of two";

    return 0;
}
