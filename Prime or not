#include<iostream>
using namespace std;
//timecomplexity=o(n)
//spacecomplexity=o(1)
void prime(int n){
    int i=2;
    bool isPrime=true;
    while(i<=n-1){
        if(n%i==0){
            isPrime=false;
            break;
        }
        i++;
        
    }
    if(isPrime==true)
    cout<<"entered number is prime";
    else
    cout<<"entered number is not prime";
}

    

int main(){
    int n;
    cout<<"Enter value of n=";
    cin>>n;
  
    prime(n);
    return 0;
}
