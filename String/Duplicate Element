#include <iostream>
#include <unordered_map>  // for frequency counting
using namespace std;

int main() {
    string str;
    cout << "Enter a string: ";
    cin >> str;

    unordered_map<char, int> freq;

    // Count frequency of each character
    for (char ch : str) {
        freq[ch]++;
    }

    cout << "Duplicate characters are: ";
    bool found = false;

    // Print characters having frequency > 1
    for (auto pair : freq) {
        if (pair.second > 1) {
            cout << pair.first << " ";
            found = true;
        }
    }

    if (!found)
        cout << "No duplicates found!";

    return 0;
}
