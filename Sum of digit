#include<iostream>
using namespace std;
int sumOfDigit(int num){
    int digsum=0;
    while(num>0){
       int lastdig=num%10;
        num=num/10;
        digsum=digsum+lastdig;
    }
     return digsum;
    }
   

int main(){
    int num;
    cout<<"Enter number=";
    cin>>num;
    cout<<sumOfDigit(num)<<endl;
}
